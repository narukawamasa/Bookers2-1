<thead>
    <tr>
      <th></th>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% user.each do |user| %>
    <tr>
      <td>
          <%= image_tag user.get_profile_image(80,80) %>
      </td>
      <td>
          <%= user.name %>
      </td>
      <td>
        <%= link_to "Show", user_path(user.id) %>
      </td>
    </tr>
    <% end %>
</tbody>
